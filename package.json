{
  "name": "react-body-highlighter",
  "version": "4.0.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "author": "HichamELBSI",
  "license": "MIT",
  "keywords": [
    "react",
    "body",
    "highlighter",
    "anatomy",
    "muscle",
    "svg",
    "interactive"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.0",
    "babel-jest": "^30.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-test-renderer": "^19.2.0",
    "tsup": "^8.4.0",
    "typescript": "^5.6.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "yarn@1.22.22"
}
